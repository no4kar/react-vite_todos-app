{
  "name": "task-manager_react-vite-ts-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run clean-folder && tsc -b",
    "build-dev": "npm run prebuild && vite build --mode development",
    "build": "npm run prebuild && vite build --mode production",
    "preview": "vite preview --port 5173",
    "deploy": "gh-pages -d dist",
    "clean-cache": "npm cache clean --force",
    "clean-folder": "rimraf $npm_package_config_clean_folder",
    "clean-folder-build": "npm run clean-folder --config.clean_folder=./build && tsc -b",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "config": {
    "clean_folder": "./dist"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.39",
    "rimraf": "^3.0.2",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}